<enumeration>
    <option default="true" id="_C2P_parent" name="C2P-has parent" oppositeName="_C2P-_has parent"
        parent="true" sortOrder="8">
        <rule sameType="true" />
    </option>
    <option description="An Operational Activity Refines an Operational Activity" id="_C2P_refines"
        name="C2P-refines" oppositeName="_C2P-_refines" sortOrder="63">
        <rule from="_C2P_operationalActivity" sameType="true" />
    </option>
    <option description="Capability beschrieben von der FC" id="_C2P_describes" name="C2P-describes"
        oppositeName="_C2P-_described" sortOrder="69">
        <rule from="_C2P_functionalChain" to="_C2P_systemCapability" />
        <rule from="_C2P_isScenario" to="_C2P_capabilityRealization" />
    </option>
    <option description="Function beteiligt an der SFC" id="_C2P_involves" name="C2P-involves"
        oppositeName="_C2P-_involves" sortOrder="72">
        <rule from="_C2P_functionalChain" to="_C2P_systemFunction" />
        <rule from="_C2P_functionalChain" sameType="true" />
        <rule from="_C2P_functionalChain" to="_C2P_functionalExchange" />
        <rule from="_C2P_isScenario" to="_C2P_logicalComponent" />
        <rule from="_C2P_isScenario" to="_C2P_logicalActor" />
    </option>
    <option description="SC welche die Funktion ausführt" id="_C2P_isPerformedBy"
        name="C2P-is performed by" oppositeName="_C2P-_is performed by" sortOrder="74">
        <rule from="_C2P_systemFunction" to="_C2P_systemComponent, _C2P_systemActor" />
        <rule from="_C2P_logicalFunction" to="_C2P_logicalComponent" />
        <rule from="_C2P_logicalFunction" to="_C2P_logicalActor" />
    </option>
    <option description="Subfunktionen" id="_C2P_subfunction" name="C2P-subfunction"
        oppositeName="_C2P-_subfunction" sortOrder="75">
        <rule from="_C2P_systemFunction, _C2P_logicalFunction" sameType="true" />
    </option>
    <option id="_C2P_realizes" name="C2P-realizes" oppositeName="_C2P-_realizes" sortOrder="76">
        <rule from="_C2P_systemFunction" to="_C2P_operationalActivity" />
        <rule from="_C2P_functionalExchange" to="_C2P_operationalInteraction" />
        <rule from="_C2P_logicalFunction" to="_C2P_systemFunction" />
        <rule from="_C2P_logicalActor" to="_C2P_systemActor" />
        <rule from="_C2P_capabilityRealization" to="_C2P_systemCapability" />
        <rule from="_C2P_functionalExchange" sameType="true" />
    </option>
    <option description="SFE welche von der SBE alloziert werden" id="_C2P_allocated"
        name="C2P-allocated" oppositeName="_C2P-_allocated" sortOrder="77">
        <rule from="_C2P_systemFunction" to="_C2P_systemActor, _C2P_systemComponent" />
        <rule from="_C2P_logicalFunction" to="_C2P_logicalActor, _C2P_logicalComponent" />
        <rule from="_C2P_logicalComponent" to="_C2P_logicalComponent" />
    </option>
    <option id="_C2P_source" name="C2P-source" oppositeName="_C2P-_source" sortOrder="78">
        <rule from="_C2P_operationalActivity" to="_C2P_operationalInteraction" />
        <rule
            from="_C2P_logicalActor, _C2P_logicalComponent, _C2P_systemActor, _C2P_systemComponent"
            to="_C2P_componentExchange, _C2P_communicationMean" />
    </option>
    <option id="_C2P_target" name="C2P-target" oppositeName="_C2P-_target" sortOrder="79">
        <rule from="_C2P_operationalActivity" to="_C2P_operationalInteraction" />
        <rule
            from="_C2P_logicalActor, _C2P_logicalComponent, _C2P_systemActor, _C2P_systemComponent"
            to="_C2P_componentExchange, _C2P_communicationMean" />
        <rule from="_C2P_logicalFunction, _C2P_systemFunction" to="_C2P_functionalExchange" />
    </option>
    <option id="_C2P_ownedMessages" name="C2P-owned messages" oppositeName="_C2P-_owned messages"
        sortOrder="82">
        <rule from="_C2P_isScenario" to="_C2P_sequenceMessage" />
    </option>
    <option id="_C2P_references" name="C2P-references" oppositeName="_C2P-_references"
        sortOrder="83">
        <rule from="_C2P_isScenario" sameType="true" />
    </option>
    <option id="_C2P_exchange_items" name="C2P-carries" oppositeName="_C2P-_carries" sortOrder="84">
        <rule
            from="_C2P_functionalExchange, _C2P_componentExchange , _C2P_logicalFunctionalExchange, _C2P_systemFunctionalExchange"
            to="_C2P_exchangeItem" />
    </option>
    <option id="_C2P_requires" name="C2P-requires" oppositeName="_C2P-_requires" sortOrder="85">
        <rule from="_C2P_logicalComponent" to="_C2P_logicalInterface" />
        <rule from="_C2P_logicalActor" to="_C2P_logicalInterface" />
    </option>
    <option id="_C2P_provides" name="C2P-provides" oppositeName="_C2P-_provides" sortOrder="86">
        <rule from="_C2P_logicalComponent" to="_C2P_logicalInterface" />
        <rule from="_C2P_logicalActor" to="_C2P_logicalInterface" />
    </option>
    <option id="_C2P_interfaces" name="C2P-interfaces" oppositeName="_C2P-_interfaces"
        sortOrder="87">
        <rule from="_C2P_logicalComponent" to="_C2P_logicalBiDirectionalInterface" />
        <rule from="_C2P_logicalActor" to="_C2P_logicalBiDirectionalInterface" />
    </option>
    <option id="_C2P_allocated_functional_exchanges" name="C2P-allocates"
        oppositeName="_C2P-_allocates"
        sortOrder="88">
        <rule from="_C2P_componentExchange, _C2P_communicationMean"
            to="_C2P_functionalExchange, _C2P_operationalInteraction" />
    </option>
    <option id="_C2P_contains" name="C2P-contains" oppositeName="_C2P-_contains" sortOrder="89">
        <rule from="_C2P_exchangeItem" to="_C2P_class" />
    </option>
    <option id="_C2P_association" name="C2P-association" oppositeName="_C2P-_association"
        sortOrder="90">
        <rule from="_C2P_class" sameType="true" />
    </option>
    <option id="_C2P_superClass" name="C2P-super class" oppositeName="_C2P-_super class"
        sortOrder="91">
        <rule from="_C2P_class" sameType="true" />
    </option>
    <option id="_C2P_groups" name="C2P-groups" oppositeName="_C2P-_groups" sortOrder="93">
        <rule from="_C2P_logicalBiDirectionalInterface" to="_C2P_logicalInterface" />
        <rule from="_C2P_logicalInterface" to="_C2P_exchangeItem" />
    </option>
    <option id="_C2P_sendingComponent" name="C2P-sending component"
        oppositeName="_C2P-_sending component"
        sortOrder="94">
        <rule from="_C2P_sequenceMessage" to="_C2P_logicalComponent" />
    </option>
    <option id="_C2P_sendingActor" name="C2P-sending actor" oppositeName="_C2P-_sending actor"
        sortOrder="95">
        <rule from="_C2P_sequenceMessage" to="_C2P_logicalActor" />
    </option>
    <option id="_C2P_receivingComponent" name="C2P-receiving component"
        oppositeName="_C2P-_receiving component" sortOrder="96">
        <rule from="_C2P_sequenceMessage" to="_C2P_logicalComponent" />
    </option>
    <option id="_C2P_receivingActor" name="C2P-receiving actor" oppositeName="_C2P-_receiving actor"
        sortOrder="97">
        <rule from="_C2P_sequenceMessage" to="_C2P_logicalActor" />
    </option>
    <option id="_C2P_invokes" name="C2P-invokes" oppositeName="_C2P-_invokes" sortOrder="98">
        <rule from="_C2P_sequenceMessage" to="_C2P_exchangeItem" />
    </option>
    <option id="_C2P_isRelatedTo" name="C2P-is related to" oppositeName="_C2P-_is related to"
        sortOrder="99">
        <rule from="_C2P_sequenceMessage" to="_C2P_logicalInterface" />
    </option>
    <option id="_C2P_view" name="C2P-view" oppositeName="_C2P-_view" sortOrder="104">
        <rule from="_C2P_systemFunction, _C2P_functionalChain, _C2P_systemCapability"
            to="diagram" />
    </option>
    <option id="_C2P_subsystem" name="C2P-subsystem" oppositeName="_C2P-_subsystem" sortOrder="105">
        <rule from="_C2P_logicalComponent" sameType="true" />
    </option>
    <option description="OA Capability involviert diese Activität" id="_C2P_involved_activities"
        name="C2P-involves" oppositeName="_C2P-_involves" sortOrder="107">
        <rule from="_C2P_entity" to="_C2P_operationalActivity" />
    </option>
    <option description="OA Capability involviert diese Entität" id="_C2P_involved_entities"
        name="C2P-involves" oppositeName="_C2P-_involves" sortOrder="108">
        <rule from="_C2P_operationalCapability" to="_C2P_entity" />
    </option>
    <option id="_C2P_description_reference" name="C2P-references in description"
        oppositeName="_C2P-_references in description" sortOrder="109">
        <rule sameType="true" />
    </option>
    <option description="Operational Entity allocates these Operational Activities."
        id="_C2P_activities" name="C2P-allocates" oppositeName="_C2P-_allocates" sortOrder="110">
        <rule from="_C2P_operationalEntity" to="_C2P_operationalActivity" />
    </option>
    <option
        description="An OperationalActivity has incoming OperationalInteractions which transport input data/exchange items."
        id="_C2P_inputs" name="C2P-incoming" oppositeName="_C2P-_incoming" sortOrder="111">
        <rule from="_C2P_operationalActivity" to="_C2P_operationalInteraction" />
    </option>
    <option
        description="An OperationalActivity has outgoing OperationalInteractions which transport output data/exchange items."
        id="_C2P_outputs" name="C2P-outgoing" oppositeName="_C2P-_outgoing" sortOrder="112">
        <rule from="_C2P_operationalActivity" to="_C2P_operationalInteraction" />
    </option>
    <option description="The exchanges connecting the Function." id="_C2P_exchanges"
        name="C2P-exchange"
        oppositeName="_C2P-_exchange" sortOrder="110">
        <rule from="_C2P_systemFunction, _C2P_logicalFunction, _C2P_physicalFunction"
            to="_C2P_functionalExchange" />
    </option>
    <option
        description="Incoming FunctionalExchange on any Function outside of the Operational layer."
        id="_C2P_input_exchanges" name="C2P-incoming" oppositeName="_C2P-_incoming" sortOrder="113">
        <rule from="_C2P_systemFunction, _C2P_logicalFunction" to="_C2P_functionalExchange" />
    </option>
    <option
        description="Outgoing FunctionalExchange on any Function outside of the Operational layer."
        id="_C2P_output_exchanges" name="C2P-outgoing" oppositeName="_C2P-_outgoing" sortOrder="114">
        <rule from="_C2P_systemFunction, _C2P_logicalFunction" to="_C2P_functionalExchange" />
    </option>
    <option description="Allocated Functions on a Component." id="_C2P_allocated_functions"
        name="C2P-allocates" oppositeName="_C2P-_allocates" sortOrder="115">
        <rule from="_C2P_systemComponent, _C2P_logicalComponent, _C2P_logicalActor"
            to="_C2P_systemFunction, _C2P_logicalFunction" />
    </option>
    <option description="Constraints on an object." id="_C2P_constraints" name="C2P-constraint by"
        oppositeName="_C2P-_constraint by">
        <rule from="_C2P_operationalCapability, _C2P_systemCapability, _C2P_capabilityRealization"
            to="_C2P_constraint" />
    </option>
    <option description="Scenarios under this Capability." id="_C2P_scenarios" name="C2P-scenario"
        oppositeName="_C2P-_scenario" sortOrder="112">
        <rule from="_C2P_capability, _C2P_capabilityRealization" to="_C2P_scenario" />
    </option>
    <option description="Linking to elements viewable in a diagram." id="_C2P_diagram_elements"
        name="C2P-shows" oppositeName="_C2P-_shows">
        <rule from="diagram" />
    </option>
    <option description="A Function realizes a lower-layer Function." id="_C2P_realized_functions"
        name="C2P-realized" oppositeName="_C2P-_realized">
        <rule from="_C2P_physicalFunction" to="_C2P_logicalFunction" />
        <rule from="_C2P_logicalFunction" to="_C2P_systemFunction" />
        <rule from="_C2P_systemFunction" to="_C2P_operationalActivity" />
    </option>
    <option description="Capability involves FunctionalChains" id="_C2P_involved_chains"
        name="C2P-involved_chains" oppositeName="_C2P-_involved_chains">
        <rule from="_C2P_operationalCapability, _C2P_systemCapability, _C2P_capabilityRealization" to="_C2P_functionalChain" />
    </option>
    <option description="Capability involves Components" id="_C2P_involved_components"
        name="C2P-involved_components" oppositeName="_C2P-_involved_components">
        <rule from="_C2P_operationalCapability" to="_C2P_entity" />
        <rule from="_C2P_systemCapability" to="_C2P_systemComponent, _C2P_systemActor" />
        <rule from="_C2P_capabilityRealization" to="_C2P_logicalComponent, _C2P_logicalActor, _C2P_physicalActor, _C2P_physicalComponent, _C2P_physicalComponentNode, _C2P_physicalComponentBehavior" />
    </option>
</enumeration>